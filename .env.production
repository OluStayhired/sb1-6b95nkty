# Production Environment Variables
# Supabase Configuration
VITE_SUPABASE_URL=https://your-production-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_production_anon_key_here

# App Configuration
VITE_APP_NAME=SchedulePro
VITE_APP_URL=https://your-domain.com
VITE_APP_ENV=production

# API Configuration
VITE_API_BASE_URL=https://api.your-domain.com

# Email Service
VITE_EMAIL_SERVICE_API_KEY=your_production_email_api_key

# SMS Service
VITE_SMS_SERVICE_API_KEY=your_production_sms_api_key

# Payment Processing
VITE_STRIPE_PUBLISHABLE_KEY=your_production_stripe_key

# Analytics
VITE_GOOGLE_ANALYTICS_ID=your_production_analytics_id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_SMS_NOTIFICATIONS=true

# Production Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error